  "main": "lib/index.js",
  "typings": "./index.d.ts",
  "author": "LÃ©o Le Bras <leolebras@wino.fr> (https://github.com/winoteam/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winoteam/react-router-navigation.git"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:flow",
    "build:watch": "babel -d ./lib ./src/ --ignore __tests__ --watch",
    "build:cjs": "babel -d ./lib ./src/ --ignore __tests__",
    "build:flow": "for file in $(find ./src -name '*.js' -not -path '*/__tests__*'); do cp \"$file\" `echo \"$file\" | sed 's/\\/src\\//\\/lib\\//g'`.flow; done",
    "build:dts": "tsc --allowJs --emitDeclarationOnly --outDir ./lib"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "router",
    "navigation",
    "navigator"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-router": "4.2.x"
  },
  "dependencies": {
    "@ryverapp/react-router-navigation-core": "file:../react-router-navigation-core",
    "react-native-tab-view": "1.2.0",
    "react-navigation": "1.5.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-router": "4.2.0"
  }
}
